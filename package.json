    "express-rate-limit": "^6.9.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "http-status-codes": "^2.2.0",
    "ioredis": "^5.3.2",
    "joi": "^17.9.2",
    "morgan": "^1.10.0",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^5.1.1",
    "winston": "^3.10.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-google-oauth20": "^2.0.0",
