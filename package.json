    "prettier": "^3.0.0",
    "prisma": "^5.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/speakeasy": "^2.0.7",
