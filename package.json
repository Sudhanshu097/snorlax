{
  "name": "nodejs-production-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "docker:build": "docker build -t nodejs-production-app .",
    "docker:run": "docker run -p 3000:3000 nodejs-production-app",
    "migrate": "prisma migrate deploy",
    "swagger": "tsoa spec-and-routes"
  },
  "dependencies": {
